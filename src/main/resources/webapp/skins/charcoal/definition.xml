<skin>
 	<name>blue</name>
 	<iconset>light</iconset>
 	<size>890,640</size>
 	<margin>25,1,0,1</margin>
 	<margincompact>25,1,25,1</margincompact>
 	<marginkiosk>1,0,0,1</marginkiosk>
 	<marginpopup>10,5,10,5</marginpopup>
	<bgstyle>#101743</bgstyle>
	<styles>
		<set name="button">
			<style name="default" value="" alt="default1" />
			<style name="red" value="red" alt="r" />
			<style name="green" value="green" alt="g" />
			<style name="blue" value="blue" alt="b" />
			<style name="yellow" value="yellow" alt="y" />
			<style name="gray" value="gray" alt="gr" />
			<style name="glass" value="glass" />

			<style name="default2" value="default2" hidden="true" />
			<style name="default3" value="default3" hidden="true" />
			<style name="highlight" value="highlight" hidden="true" />
			<style name="alert" value="alert" hidden="true" />
		</set>
		<set name="panel">
			<style name="default" value="" alt="default1" />
			<style name="red" value="red" alt="r" />
			<style name="green" value="green" alt="g" />
			<style name="blue" value="blue" alt="b" />
			<style name="yellow" value="yellow" alt="y" />
			<style name="gray" value="gray" alt="gr" />
			<style name="black" value="black" />

			<style name="default2" value="default2" hidden="true" />
			<style name="default3" value="default3" hidden="true" />
			<style name="highlight" value="highlight" hidden="true" />
			<style name="alert" value="alert" hidden="true" />
		</set>
	</styles>
 </skin>